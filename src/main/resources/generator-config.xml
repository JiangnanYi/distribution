<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="my" targetRuntime="MyBatis3">

        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="supperessAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="org.mariadb.jdbc.Driver"
                          connectionURL="jdbc:mariadb://localhost:3306/LogisticsDB"
                          userId="root"
                          password="990801"/>

        <javaModelGenerator targetPackage="com.lij.entity" targetProject="E:/二学年/diea练习/distribution/src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper"  targetProject="E:/二学年/diea练习/distribution/src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.lij.dao"  targetProject="E:/二学年/diea练习/distribution/src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <table tableName="%">
            <generatedKey column="id" sqlStatement="Mysql"/>
        </table>
    </context>
</generatorConfiguration>